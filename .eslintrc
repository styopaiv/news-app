{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "no-use-before-define": [0],
    "react/jsx-no-bind": [0],
    "react/prefer-stateless-function": [0],
    "react/require-default-props": [0]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
